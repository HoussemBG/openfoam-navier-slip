FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      blockMeshDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
scale 0.001;

R 32;
H 0.005;
//Ax #calc "$R * cos(0.5)";
//Ay #calc "$R * sin(0.5)";

vertices        
(
	//Axis Points
        /*(0 0 0)
	(0 0 $H)
	($Ax $Ay 0)
	($Ax $Ay $H)
	($Ax -$Ax $H)
	($Ax -$Ax 0)*/
	
	(0 0 0)
	(0 0 $H)
	(31.995 0.558 0)
	(31.995 0.558 $H)
	(31.995 -0.558 $H)
	(31.995 -0.558 0)
);

blocks          
(
	hex (0 5 2 0 1 4 3 1) (5000 1 20) simpleGrading (1 1 1)
);

edges           
(
arc 2 5($R 0 0)
arc 3 4($R 0 $H)
);

boundary
(
    top
    {
        type wall;
        faces
        (
            (1 4 3 1)
        );
    }
    
    bottom  
    {
        type wall;
        faces
        (
            (0 2 5 0)
        );
    }
    outerWall
    {
        type wall;
        faces
        (
            (3 4 5 2)
        );
    }
    left
    {
        type wedge;
        faces
        (
            (0 1 3 2)
        );
    }
    right
    {
        type wedge;
        faces
        (
            (1 0 5 4)
        );
    }
    axis
    {
        type empty;
        faces
        (
            (0 0 1 1)
        );
    }
);

mergePatchPairs
(
);
